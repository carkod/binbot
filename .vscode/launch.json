{
  "version": "0.0.2",
  "configurations": [
    {
      "name": "Python: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "cwd": "${workspaceFolder}/api",
      "args": ["main:app", "--port", "8008"],
      "justMyCode": true,
      "console": "internalConsole"
    },
    {
      "name": "Binquant: Producer",
      "type": "debugpy",
      "request": "launch",
      "program": "binquant/producer.py",
      "console": "internalConsole",
      "justMyCode": true,
      "envFile": "${workspaceFolder}/.env",
    },
    {
      "name": "Binquant: Consumer",
      "type": "debugpy",
      "request": "launch",
      "program": "binquant/consumer.py",

      "console": "internalConsole",
      "justMyCode": true
    },
    {
      "name": "Python: streaming",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/api/market_updates.py",
      "console": "internalConsole",
      "justMyCode": false
    },
    {
      "name": "Python: cronjobs",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/api/cronjobs.py",
      "console": "internalConsole",
      "justMyCode": true
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Web",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/web/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
      "outFiles": [
        "${workspaceFolder}/web/**/*.js",
        "${workspaceFolder}/web/src/node_modules/**"
      ]
    },
    {
      "name": "Python: Research",
      "type": "debugpy",
      "request": "launch",
      "program": "binbot-research/__init__.py",
      "console": "internalConsole"
    }
  ]
}
